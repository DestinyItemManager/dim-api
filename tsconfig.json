{
  "compilerOptions": {
    "esModuleInterop": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "strictBindCallApply": true,
    "module": "NodeNext",
    "target": "es2022",
    "moduleResolution": "NodeNext",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  }
}
